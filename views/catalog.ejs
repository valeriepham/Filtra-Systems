</head>

<body>
  <!--header-->
  <header>
    <!-- navbar -->
    <nav class="navbar bg-primary" style="background-color: #00aeef; height:20%">
      <a class="navbar-brand" href="/">
        <img src="http://www.fsbagfilter.com//wp-content/uploads/2016/06/footerlogo.png" height:20%>
      </a>
    </nav>

    <ul class="nav nav-pills nav-fill bg-primary">
      <li class="nav-item">
        <a class="nav-link active" href="\">Home</a>
      </li>
      <li class="nav-item">
        <a class="nav-link active" href="#">About Us</a>
      </li>
      <li class="nav-item">
        <a class="nav-link active" href="/catalog">Catalogue</a>
      </li>
      <li class="nav-item">
        <a class="nav-link active" href="#">Request Quote</a>
      </li>
      <li class="nav-item">
        <a class="nav-link active" href="#">News</a>
      </li>
      <li class="nav-item">
