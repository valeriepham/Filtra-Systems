      </li>
    </ul>

    <!-- <img src="http://www.go-dove.com/shared/auction/images/photos/85779/85778895.jpg" style="width:100%; height:80%"> -->

    <div class="jumbotron">
      JUMBOTRON
    </div>
    <div class="content container">
      CONTENT
      <div class="selection">
        SELECTION PANE
        <div class="search">
          <form>
            <button class="icon">
              <i class="fa fa-search"></i>
            </button>
            <input type="search" name="search" id="searchbar" placeholder="Search..." />
          </form>
        </div>
        <div class="filter">
          FILTERING
          <ul class="categories">
            <li>Category 1</li>
            <ul class="filters">
              <li>Filter 1.1</li>
              <li>Filter 1.2</li>
              <li>Filter 1.3</li>
            </ul>
            <li>Category 2</li>
            <ul class="filters">
              <li>Filter 2.1</li>
              <li>Filter 2.2</li>
              <li>Filter 2.3</li>
            </ul>
            <li>Category 3</li>
            <ul class="filters">
              <li>Filter 3.1</li>
              <li>Filter 3.2</li>
              <li>Filter 3.3</li>
            </ul>
          </ul>
        </div>
      </div>
      <div class="product-listing">
        LISTING
        <% chunks.forEach(function(chunk){ %>
          <div class="row">
            <% chunk.forEach(function(series){ %>
              <div class="col-sm-6 col-md-3">
                <div class="thumbnail">
                  <a href="products/<%= series %>">
                    <img src="images/<%=series%>.jpg" class="img-responsive" alt="<%=series%>">
                    <div class="caption">
                      <%= series %>
                        <p class="description">Blah Blah Blah</p>
                    </div>
                  </a>
                </div>
              </div>
              <%}) %>
          </div>
          <%}) %>
      </div>
    </div>

    <!--footer-->
    <footer id="myFooter">
      <div class="container">
        <div class="row">
          <div class="col-sm-3 myCols">
            <h5>Get started</h5>
